    screenPosition: 0
    characterImage: 1
    nameCharacter: 1
    textShow: "\u042F\u043A \u0432\u0430\u043C \u0441\u043A\u0430\u0437\u0430\u0442\u0438,
      \u0417\u0430\u0445\u0430\u0440\u0435\u0441\u0435. \u0422\u0443\u0442 \u043B\u0435\u0433\u0448\u0435
      \u0431\u0443\u043B\u043E \u0431 \u0432\u0438\u0431\u0440\u0430\u0442\u0438,
      \u0443 \u0447\u043E\u043C\u0443 \u044F \u0439\u043E\u0433\u043E \u041D\u0415
      \u043F\u0456\u0434\u043E\u0437\u0440\u044E\u044E. \u041F\u0440\u043E\u0442\u0435
      \u0441\u0430\u043C\u0435 \u0441\u0442\u043E\u0441\u043E\u0432\u043D\u043E \u0432\u0431\u0438\u0432\u0441\u0442\u0432\u0430
      \u041A\u043E\u0440\u043D\u0456\u044F \u043F\u043E\u043A\u0438 \u0449\u043E
      \u0441\u043A\u0430\u0437\u0430\u0442\u0438 \u0441\u043A\u043B\u0430\u0434\u043D\u043E."
    actionsTypeEnd: 
  - actionsType: 
    musicTypeOnStart: 0
    statusSetter:
      enable: 0
      statusesValues: []
    screenPosition: 0
    characterImage: 3
    nameCharacter: 3
    textShow: "\u042F \u043F\u0430\u043C'\u044F\u0442\u0430\u044E, \u044F\u043A \u041B\u0435\u0441\u044C
      \u0448\u0435 \u043C\u0430\u043B\u0438\u0439 \u0431\u0443\u0432. \u041F\u043E
      \u044E\u043D\u043E\u0441\u0442\u0456 \u0437\u043E\u0432\u0441\u0456\u043C \u043C\u0440\u0456\u0439\u043D\u0438\u043A.
      \u0410 \u0439 \u0442\u043E\u0447\u043D\u043E! \u0417\u0431\u0438\u0440\u0430\u0432\u0441\u044C
      \u043F\u043E\u043B\u0456\u0446\u0456\u044F\u043D\u0442\u043E\u043C \u0431\u0443\u0442\u044C,
      \u043F\u0440\u044F\u043C \u044F\u043A \u0442\u0438. \u041A\u0430\u0437\u0430\u0432,
      \u0448\u043E \u0445\u043E\u0447\u0435 \u0434\u043E\u0431\u0440\u0435 \u043B\u044E\u0434\u044F\u043C
