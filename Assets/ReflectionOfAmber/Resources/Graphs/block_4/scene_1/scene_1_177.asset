      statusesValues: []
    screenPosition: 0
    characterImage: 5
    nameCharacter: 5
    textShow: "\u0422\u043E\u0436 \u044F \u0434\u043E\u0431\u0440\u0435 \u043A\u043E\u043D\u0442\u0440\u043E\u043B\u044E\u044E
      \u0446\u0435\u0439 \u043F\u0440\u043E\u0446\u0435\u0441 \u0456 \u043D\u0430\u0432\u0456\u0442\u044C
      \u0437\u043C\u043E\u0433\u043B\u0430 \u043F\u043E\u043A\u0430\u0437\u0430\u0442\u0438
      \u0442\u043E\u0431\u0456 \u0432\u0456\u0434\u0431\u043B\u0438\u0441\u043A\u0438
      \u0456\u043D\u0448\u0438\u0445 \u043F\u0440\u043E\u0441\u0442\u043E\u0440\u0456\u0432.
      \u0429\u043E\u043F\u0440\u0430\u0432\u0434\u0430, \u0442\u0438 \u043D\u0430\u0437\u0432\u0430\u043B\u0430
      \u0446\u0435 \u0433\u0430\u043B\u044E\u043D\u0430\u043C\u0438, \u0430\u043B\u0435\u2026
      \u0413\u0430\u0440\u0430\u0437\u0434, \u044F \u0431 \u043D\u0430\u0437\u0432\u0430\u043B\u0430
      \u0442\u0430\u043A \u0441\u0430\u043C\u043E."
    endOfText: 1
    actionsTypeEnd: 
